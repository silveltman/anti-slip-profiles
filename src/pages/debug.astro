---
import Layout from '@layouts/Layout.astro'
import { getProducts } from '@utils/shopify'
import { Icon } from 'fulldev-ui'

const products = await getProducts()
const mappedProducts = products.map((prod) => {
  return {
    title: prod.title,
    handle: prod.handle,
  }
})
// console.log(mappedProducts)
---

<Layout seo={null}>
  <i class="material-symbols-rounded material-symbols-outlined">check_circle</i>
  <Icon
    size={48}
    fill
  />
  <Icon
    name="car"
    size={48}
    fill
  />
  <Icon
    size={48}
    fill
  />
  <Icon
    name="location_on"
    size={48}
    fill
  />
</Layout>
