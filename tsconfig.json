{
  "extends": "astro/tsconfigs/base",
  "compilerOptions": {
    "strictNullChecks": true,
    "baseUrl": ".",
    "paths": {
      "@layouts/*": [
        "src/layouts/*"
      ],
      "@components/*": [
        "src/components/*"
      ],
      "@data/*": [
        "src/data/*"
      ],
      "@content/*": [
        "src/content/*"
      ],
      "@utils/*": [
        "src/utils/*"
      ],
      "@stores/*": [
        "src/stores/*"
      ]
    },
  },
  "include": ["src/**/*", "node_modules/fulldev-ui/dist/**/*.d.ts"],
}